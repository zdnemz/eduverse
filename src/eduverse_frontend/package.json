    "@hookform/resolvers": "^5.2.1",
    "@tailwindcss/vite": "^4.1.11",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.6",
    "lucide-react": "^0.525.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.7.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
